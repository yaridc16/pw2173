      'Al Shabaan',
      'arse',
      'chechen terrorists',
      'coon',
      'Shabaab'
    ];
    // Whitelist for excluded domains
    var whitelist = [];
    var dataToCheck = prepareDataToCheck();
    var debug = window.localStorage.epBlacklist === '1';

    // Logic for page checking
    function shouldHide(badKeywords, dataToCheck) {
      return badKeywords.some(function (keyword) {
        var reg = new RegExp('\\b' + keyword + '\\b', 'i');
        return dataToCheck.match(reg);
      });
    }

    function prepareDataToCheck() {
      var documentText = document.body.innerText;
      var pageMetaRawKeywords = document.querySelector('meta[name=keywords]');
      var pageMetaKeywords = pageMetaRawKeywords ? pageMetaRawKeywords.content.split(',') : [];
      return documentText.concat(pageMetaKeywords);
    }

    function logProhibitedKeywords(badKeywords, dataToCheck) {
      badKeywords.forEach(function (keyword) {
        var reg = new RegExp('\\b' + keyword + '\\b', 'i');
        if (dataToCheck.match(reg)) {
          console.log('[Prohibited Keyword]:', keyword);
        }
      });
    }

    function checkIfURLIsWhitelisted() {
      var url = window.location.hostname + window.location.pathname;
      if (url.indexOf('www.') < 0) {
        url = 'www.' + url;
      }
      return whitelist.some(function (entry) {
        return url.match(new RegExp(entry.replace('*', '.*') + '$'));
      });
    }

    function checkPageAndRemovePlayers() {
      if (shouldHide(badKeywords, dataToCheck)) {
        var msgObj = { playerEvent: 'removePlayers' };
        var msg = JSON.stringify(msgObj);
        if (debug) {
          logProhibitedKeywords(badKeywords, dataToCheck);
        }
        window.postMessage(msg, '*');
      }
    }

    function parseWhitelist(whitelistObject) {
      var currentDomain = window.location.hostname;
      for (var domain in whitelistObject) {
        if (whitelistObject.hasOwnProperty(domain) && domain.indexOf(currentDomain) > -1) {
          whitelist = whitelistObject[domain];
        }
      }
      init();
    }

    function setUpWhitelist() {
      var script = document.createElement('script');
      script.src = '//player.performgroup.com/eplayer/assets/urls.json';
      document.head.appendChild(script);
      script.parentNode.removeChild(script);
    }

    function init() {
      if (checkIfURLIsWhitelisted()) {
        if (debug) {
          console.log('[Keyword Checker]: URL is whitelisted');
        }
      } else {
        checkPageAndRemovePlayers();
        if (debug) {
          console.log('[Keyword Checker]: URL is not whitelisted');
        }
      }
    }

    return {
      setUpWhitelist: setUpWhitelist,
      parseWhitelist: parseWhitelist
    };
  };

  window.KC = keywordChecker();
  //window.KC.setUpWhitelist();

}(window));