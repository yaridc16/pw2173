<script id="navegg" type="text/javascript" src="https://tag.navdmp.com/tm40443.js" defer></script><script src="https://www.googletagservices.com/tag/js/gpt.js" async="async"></script>





  

<script>



var googletag = googletag || {};
googletag.cmd = googletag.cmd || [];





var gexDfp = (function (googletag) {
  var _slots = {
    body: {},
    megamenu: {},
    asArray:  function(from) { return Object.keys(_slots[from]).map(function (slot) { return _slots[from][slot] }) }
  };
  var _mapSizes = function(sizeMapping) {
    var adSizes = [];
    var defaultSize = [];
    sizeMapping.forEach(function(size) {
      var slotSizesDef = [];
      var browserSize = [ size.browser.width, size.browser.height ];

      size.adSize.forEach(function (ad, index) {
        if (index === 0 && defaultSize.length === 0) {
          defaultSize = [ad.width, ad.height];
        }
        if ((ad.fluid != null) && ad.fluid) {
          slotSizesDef.push('fluid');
        } else {
          slotSizesDef.push([ ad.width, ad.height ]);
        }
      })
      adSizes.push([browserSize, slotSizesDef]);
    });
    return { sizes: adSizes, default: defaultSize };
  };

  return {
    slots: _slots,
    add: function (target, id, slot) {
      _slots[target][id] = slot;
    },
    show: function (id) {
      if (typeof id === 'undefined') {
        var node = document.currentScript.parentNode;
        if (!node.id) { node.id = 'ad-gpt-' + Math.random().toString(36).substr(2, 5); }
        id = node.id;
      }
      var currentSlot = null
      if (_slots.body[id]) {
        currentSlot = _slots.body[id];
      } else if (_slots.megamenu[id]) {
        currentSlot = _slots.megamenu[id];
      }

      if (currentSlot) {
        if (currentSlot.getResponseInformation()) {
          console.info('[DFP] Refresh ad slot:', id);
          return googletag.cmd.push(function () {
            googletag.pubads().refresh([currentSlot]);
          });
        } else {
          return console.error('[DFP] Ad not found on first request. ', id);
        }
      }

      try {
        var node = document.getElementById(id);
        if (!node) throw new ReferenceError('Element #' + id + ' not found.');
        var ad = JSON.parse(node.dataset.dfp);
        var adUnit = ad.adUnit;
        var adType = ad.adType;
      } catch (e) {
        return console.error('[DFP] ' + e);
      }

      var mapSizes = _mapSizes(ad.sizeMapping.default || []);
      var adSizes = mapSizes.sizes;
      var defaultSize = mapSizes.default;

      var targeting = ad.targeting || {};
      var adTargeting = Object.keys(targeting).map(function (key) { return [key, targeting[key]] });

      return googletag.cmd.push(function () {
        var slot;
        if (adType === 'floating') {
          slot = googletag.defineOutOfPageSlot(adUnit, id);
        } else {
          slot = googletag.defineSlot(adUnit, defaultSize, id);
          var sizeMapping = googletag.sizeMapping();
          adSizes.forEach(function (s) {
            sizeMapping.addSize(s[0], s[1]);
          });
          slot.defineSizeMapping(sizeMapping.build());
        }

        slot.setTargeting('stage', 'production');
        adTargeting.forEach(function (t) {
          slot.setTargeting(t[0], t[1]);
        });

        slot.addService(googletag.pubads());

        
        if (adUnit.match(/mega-menu/)) {
          window.gexDfp.add('megamenu', id, slot);
        } else {
          googletag.display(id);
        
          window.gexDfp.add('body', id, slot);
        
        }
      });
    }
  };
}(googletag));


googletag.cmd.push(function() {

  googletag.pubads().collapseEmptyDivs();



/*
 * #ad-gpt-0
 * unit: /93357320/es.gex.mediotiempo.com/home
 * type : floating
 * defaultSize: [ 640, 480 ]
 * sizes: [ [ [1024, 768], [ [970, 90], ] ],[ [640, 480], [ [970, 90], ] ], ]
 */
  var slot0 = googletag.defineOutOfPageSlot('/93357320/es.gex.mediotiempo.com/home', 'ad-gpt-0')

    .addService(googletag.pubads())
    .setTargeting('contentId', '00000156-d7e9-dc36-a95e-fffd01c10000')
    .setTargeting('stage', 'production');
    gexDfp.add('body', 'ad-gpt-0', slot0);

/*
 * #ad-gpt-1
 * unit: /93357320/es.gex.mediotiempo.com/home/top-sponsor
 * type : top-sponsor
 * defaultSize: [ 640, 480 ]
 * sizes: [ [ [1024, 768], [ [1200, 30], ] ],[ [640, 480], [ [1200, 30], ] ], ]
 */
  var size1 = googletag.sizeMapping() 
    .addSize([1024, 768], [[1200, 30],])
    .addSize([640, 480], [[1200, 30],])
    .build();

  var slot1 = googletag.defineSlot('/93357320/es.gex.mediotiempo.com/home/top-sponsor', [ 640, 480 ], 'ad-gpt-1')
    .defineSizeMapping(size1) 
    .addService(googletag.pubads())
